<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Description>Entity Framework Cosmos database provider for Kledex domain store.</Description>
    <PackageTags>$(PackageTags), ef, entity framework, cosmos</PackageTags>
    <PackageLicenseExpression>$(PackageLicense)</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Cosmos" Version="6.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Kledex.Store.EF\Kledex.Store.EF.csproj" />
    <ProjectReference Include="..\Kledex\Kledex.csproj" />
  </ItemGroup>

</Project>
